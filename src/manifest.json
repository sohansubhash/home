{
  "manifest_version": 3,
  "name": "Voyager",
  "version": "0.3",
  "description": "Replaces your new tab page",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
    "developer": {
    "name": "Sohan Subhash",
    "url": "https://github.com/sohansubhash/voyager"
  },
    "icons": {
    "48": "/images/icon.svg",
    "96": "/images/icon.svg"
  },
  "permissions": [
	 "bookmarks" 
  ],
  "background": {
  "scripts": ["background.js"]
}
}
